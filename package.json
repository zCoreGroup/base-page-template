{
  "name": "portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build-prep": "cp .env.$NODE_ENV .env",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\" \"!**/*.stories.{js,jsx,ts,tsx}\" \"!.storybook/**\"",
    "check-format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\" \"!**/*.stories.{js,jsx,ts,tsx}\" \"!.storybook/**\"",
    "check-lint": "eslint .",
    "check-types": "tsc --noEmit",
    "format-all": "yarn format && yarn lint",
    "test:unit": "jest",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:schema": "ts-json-schema-generator -p 'src/types.ts' --type '*' --out 'src/schemas.json' && node scripts/updateRefs.js",
    "clean": "rm -rf node_modules && rm -rf .next && yarn cache clean",
    "prepare": "husky"
  },
  "dependencies": {
    "@directus/sdk": "^16.1.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^8.57.0",
    "@fontsource/ibm-plex-mono": "^5.0.13",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.16.0",
    "@testing-library/dom": "^10.2.0",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^4.18.3",
    "@types/validator": "^13.12.0",
    "dompurify": "^3.1.6",
    "dotenv": "^16.4.5",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lodash": "^4.17.21",
    "next": "^14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-material-ui-carousel": "^3.4.2",
    "react-slick": "^0.30.2",
    "slick-carousel": "^1.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.17.14",
    "ts-json-schema-generator": "^2.3.0",
    "typeface-libre-franklin": "^1.1.13",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.6.0",
    "@next/eslint-plugin-next": "^14.2.5",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/nextjs": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/typescript-estree": "^7.17.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "storybook": "^8.1.11",
    "storybook-zeplin": "^2.0.3",
    "ts-jest": "^29.2.4",
    "typescript": "5.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx, ts,tsx}": "yarn run format-all"
  },
  "resolutions": {
    "storybook-zeplin/@zeplin/storybook-inspector/prettier": "3.3.3"
  }
}
